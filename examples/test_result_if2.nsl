# Test if with else but simpler

fn main() {
    print("Testing if with Result:")

    let flag = 5.0
    let r = if flag > 0.0 then { ok(flag) } else { err(0.0) }

    print("Got result from if")

    match r {
        case ok(v) => print("Got ok!")
        case err(e) => print("Got err!")
    }

    print("Done!")
}
