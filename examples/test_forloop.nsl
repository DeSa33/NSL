# Test for loops with ranges

fn main() {
    print("For loop 0..5 (exclusive):")
    for i in 0..5 {
        print(i)
    }

    print("For loop 0..=5 (inclusive):")
    for i in 0..=5 {
        print(i)
    }

    # Sum using for loop
    let sum = 0.0
    for i in 1..=10 {
        print(i)
    }
    print("Sum printed 1 to 10")

    print("Done!")
}
