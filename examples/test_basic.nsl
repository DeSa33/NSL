// Test basic NSL functionality

// Variables
let x = 10
let y = 20
print("x =", x)
print("y =", y)

// Arithmetic
let sum = x + y
print("x + y =", sum)

// Function definition
fn multiply(a, b) {
    return a * b
}

let product = multiply(x, y)
print("x * y =", product)

// Control flow
if (x < y) {
    print("x is less than y")
} else {
    print("x is not less than y")
}

// For loop
print("Counting from 1 to 5:")
for i in range(1, 6) {
    print(i)
}

print("All basic tests passed!")
