# More comprehensive array tests

fn main() {
    # Test with integer literals (will be stored as doubles internally)
    let nums = [10, 20, 30, 40, 50]

    print("Integer array:")
    print(nums[0])
    print(nums[4])

    # Test array in a function
    print("First element:")
    let first = nums[0]
    print(first)

    # Test arithmetic with indexed values
    print("Multiply elements:")
    let product = nums[0] * nums[1]
    print(product)

    # Nested index expression (just arithmetic)
    let idx = 2
    print("Element at computed index 2:")
    print(nums[2])

    print("Tests passed!")
}
