print("min([5,2,8,1,9]) =", min(arr))
print("max([5,2,8,1,9]) =", max(arr))
print("")

// Test 10: Sign Function
print("-- Sign Function --")
print("sign(-10) =", sign(-10))
print("sign(0) =", sign(0))
print("sign(10) =", sign(10))
print("")

