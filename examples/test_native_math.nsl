print("E =", E)
print("")

// Test 2: Square Root
print("-- Square Root --")
print("sqrt(16) =", sqrt(16))
print("sqrt(2) =", sqrt(2))
print("sqrt(144) =", sqrt(144))
print("")

// Test 3: Trigonometric Functions
print("-- Trigonometric Functions --")
print("sin(0) =", sin(0))
print("sin(PI/2) =", sin(PI / 2))
print("cos(0) =", cos(0))
print("cos(PI) =", cos(PI))
print("tan(0) =", tan(0))
print("tan(PI/4) =", tan(PI / 4))
print("")

// Test 4: Inverse Trigonometric Functions
print("-- Inverse Trigonometric Functions --")
print("asin(0) =", asin(0))
print("asin(1) =", asin(1))
print("acos(1) =", acos(1))
print("acos(0) =", acos(0))
print("atan(0) =", atan(0))
print("atan(1) =", atan(1))
print("atan2(1, 1) =", atan2(1, 1))
print("")

// Test 5: Absolute Value
print("-- Absolute Value --")
print("abs(-5) =", abs(-5))
print("abs(5) =", abs(5))
print("abs(-3.14) =", abs(-3.14))
print("")

// Test 6: Rounding Functions
print("-- Rounding Functions --")
print("floor(3.7) =", floor(3.7))
print("floor(-3.7) =", floor(-3.7))
print("ceil(3.2) =", ceil(3.2))
print("ceil(-3.2) =", ceil(-3.2))
print("round(3.5) =", round(3.5))
print("round(3.4) =", round(3.4))
print("round(3.14159, 2) =", round(3.14159, 2))
print("")

// Test 7: Logarithmic Functions
print("-- Logarithmic Functions --")
print("log(E) =", log(E))
print("log(100, 10) =", log(100, 10))
print("log10(100) =", log10(100))
print("log10(1000) =", log10(1000))
print("")

// Test 8: Exponential Functions
print("-- Exponential Functions --")
print("exp(0) =", exp(0))
print("exp(1) =", exp(1))
print("pow(2, 10) =", pow(2, 10))
print("pow(3, 3) =", pow(3, 3))
print("")

// Test 9: Min and Max
print("-- Min and Max --")
print("min(1, 2, 3) =", min(1, 2, 3))
print("max(1, 2, 3) =", max(1, 2, 3))
