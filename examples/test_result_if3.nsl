# Test function with if returning Result

fn get_ok_result() {
    ok(42.0)
}

fn get_err_result() {
    err(0.0)
}

fn get_conditional_result(flag: number) {
    if flag > 0.0 then {
        get_ok_result()
    } else {
        get_err_result()
    }
}

fn main() {
    print("Testing conditional Result:")

    let r1 = get_conditional_result(5.0)
    print("get_conditional_result(5.0):")
    match r1 {
        case ok(v) => print("Got ok!")
        case err(e) => print("Got err!")
    }

    print("Done!")
}
