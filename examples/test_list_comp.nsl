# Test list comprehensions

fn main() {
    print("Testing list comprehensions:")

    # Simple squares: [x * x for x in range(1, 6)]
    print("Squares of 1-5:")
    let squares = [x * x for x in range(1, 6)]
    print(squares[0])
    print(squares[1])
    print(squares[2])
    print(squares[3])
    print(squares[4])

    # Filtered comprehension: only even values
    print("Even numbers from 1-10:")
    let evens = [x for x in range(1, 11) if x % 2 == 0]
    print(evens[0])
    print(evens[1])
    print(evens[2])
    print(evens[3])
    print(evens[4])

    # Expression in comprehension
    print("Doubled values of 1-5:")
    let doubled = [x * 2 for x in range(1, 6)]
    print(doubled[0])
    print(doubled[1])
    print(doubled[2])
    print(doubled[3])
    print(doubled[4])

    print("Done!")
}
