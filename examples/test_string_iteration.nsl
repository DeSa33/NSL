// Test String Iteration in NSL
print("=== Testing String Iteration ===")
print("")

// Test 1: Basic string iteration
print("-- Test 1: Basic String Iteration --")
for c in "Hello" {
    print("Char:", c)
}
print("")

// Test 2: Building a string from characters
print("-- Test 2: Reverse String --")
let original = "NSL"
let reversed = ""
for c in original {
    reversed = c + reversed
}
print("Original:", original)
print("Reversed:", reversed)
print("")

// Test 3: Count characters
print("-- Test 3: Count Vowels --")
let text = "Hello World"
let vowels = 0
for c in text {
    if (c == "a" or c == "e" or c == "i" or c == "o" or c == "u" or
        c == "A" or c == "E" or c == "I" or c == "O" or c == "U") {
        vowels = vowels + 1
    }
}
print("Text:", text)
print("Vowel count:", vowels)
print("")

// Test 4: String with break
print("-- Test 4: Find Character with Break --")
let search = "Programming"
let found = false
let position = 0
for c in search {
    if (c == "g") {
        found = true
        break
    }
    position = position + 1
}
if (found) {
    print("Found 'g' at position:", position)
} else {
    print("'g' not found")
}
print("")

// Test 5: String with continue
print("-- Test 5: Skip Spaces --")
let sentence = "a b c"
let noSpaces = ""
for c in sentence {
    if (c == " ") {
        continue
    }
    noSpaces = noSpaces + c
}
print("Original:", sentence)
print("No spaces:", noSpaces)
print("")

print("=== String Iteration Tests Complete ===")
