// NSL Module System Test
// Tests the new import/export/module syntax

// Import styles
import math
import math::linear_algebra
import { sin, cos } from math
import * from math::constants

// Public declarations
pub fn add(a, b) {
    return a + b
}

pub let PI = 3.14159

// Module definition
module geometry {
    pub fn area(width, height) {
        return width * height
    }
}

// Main function to test
fn main() {
    let result = add(10, 20)
    print(result)
    print(PI)
}
