// Math functions test

fn int_div(a: number, b: number) {
    // Integer division using floor
    return floor(a / b)
}

fn main() {
    print("Floor tests:")
    print(floor(3.7))   // 3.0
    print(floor(-2.3))  // -3.0
    print(floor(5.0))   // 5.0

    print("Ceil tests:")
    print(ceil(3.2))    // 4.0
    print(ceil(-2.8))   // -2.0

    print("Round tests:")
    print(round(3.4))   // 3.0
    print(round(3.6))   // 4.0

    print("Integer division:")
    print(int_div(17.0, 5.0))  // 3.0
    print(int_div(10.0, 3.0))  // 3.0

    print("Other math:")
    print(sqrt(16.0))   // 4.0
    print(abs(-5.0))    // 5.0
}
