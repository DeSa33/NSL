# Edge case tests for autofix

# 1. Nested brackets with missing commas
let nested = [[1 2] [3 4] [5 6]]
let deep = [[[1 2 3]]]

# 2. Mixed content in objects
let config = { host: "localhost", port: 8080, debug: true }

# 3. Function with complex params
fn process(data, items, count) { return data }

# 4. Multiple issues on same line
fn test(a, b) { return true }

# 5. Chained Python blocks
if a > 0 {
  if b > 0 {
    print("both positive")
  } else {
    print("b not positive")
} else
} {
  print("a not positive")
}

# 6. For loop Python style
for i in range(5) {
  print(i)
}

# 7. While loop Python style  
while x > 0 {
  x = x - 1
}

# 8. Empty brackets (should not add commas)
let empty = []
let emptyObj = {}
fn noParams() { return 1 }

# 9. Single element (should not add commas)
let single = [42]
let singleObj = { x: 1 }

# 10. Already correct (should not modify)
let correct = [1, 2, 3]
fn valid(a, b, c) { return a + b + c }

print("edge cases complete")
